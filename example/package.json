    "start-webpack": "webpack serve",
    "reinstall": "yarn clean && yarn install"
  },
  "dependencies": {
    "@formatjs/intl": "2.6.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.5",
    "react-native-localize": "file:../",
    "react-native-web": "0.19.1",
    "react-native-windows": "0.71.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@types/react": "^18.0.31",
    "babel-loader": "^9.1.2",
    "metro-react-native-babel-preset": "0.73.9",
    "pod-install": "^0.1.38",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}
