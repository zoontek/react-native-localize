{
  "name": "react-native-localize-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-macos": "yarn add react-native-macos@0.68.66",
    "clean-modules": "rm -rf ./node_modules/react-native-permissions/{example,node_modules}",
    "clean": "rm -rf ./node_modules ./ios/Pods",
