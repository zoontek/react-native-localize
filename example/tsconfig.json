{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["ES2016"],
    "moduleResolution": "Node",
    "target": "ESNext",

    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
